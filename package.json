{
  "name": "kreator-templates",
  "version": "1.0.3",
  "description": "Factory cli to bootstrap custom set of projects",
  "main": "index.js",
  "files": [
    "dist/index.js",
    "README.md",
    "templates"
  ],
  "bin": {
    "factory": "./dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
